{
   "name": "浮图塔助手",
   "description": "",
   "version": "1.1",
   "manifest_version": 2,
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "48": "img/icon_48.png"
   },
   "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "browser_action": {
      "default_icon": "img/icon_128.png",
      "default_title": "浮图塔助手"
   },
   "content_scripts": [ {
      "js": [ "js/jquery-2.1.0.min.js", "js/helper.js" ],
      "matches": [ "http://cms.fututa.com/*" ],
      "run_at": "document_end"
   } ]
}
